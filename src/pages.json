        {
          "path": "detail",
          "style": {
            "navigationBarTitleText": "倒数日详情",
            "navigationStyle": "custom",
            "enablePullDownRefresh": false
          }
        }
      ]
    },
    {
      "root": "src/subpackages/edit",
