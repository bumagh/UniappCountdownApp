        {
          "path": "edit",
          "style": {
            "navigationBarTitleText": "编辑倒数日",
            "navigationStyle": "custom",
            "enablePullDownRefresh": false
          }
        }
      ]
    },
    {
      "root": "src/subpackages/login",
      "pages": [
        {
          "path": "login",
          "style": {
            "navigationBarTitleText": "登录",
            "navigationStyle": "custom",
            "enablePullDownRefresh": false
          }
        }
      ]
    }
  ],
